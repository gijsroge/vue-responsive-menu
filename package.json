{
  "name": "vue-responsive-menu",
  "version": "0.1.2",
  "description": "A renderless Vue component that will auto detect if menu items don't fit and moves them to a separate dropdown. Also known as the Priority+ pattern.",
  "author": {
    "name": "Gijs Rog√©",
    "email": "rogegijs@gmail.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve demo",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "node node_modules/@vue/cli-service/bin/vue-cli-service.js test:e2e --headless",
    "lint": "vue-cli-service lint",
    "build:npm": "rollit",
    "release": "release-it"
  },
  "main": "dist/vue-responsive-menu.cjs.js",
  "module": "dist/vue-responsive-menu.es.js",
  "browser": "dist/vue-responsive-menu.es.js",
  "unpkg": "dist/vue-responsive-menu.js",
  "files": [
    "src",
    "dist/*.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "core-js": "^3.4.4",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-e2e-cypress": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-unit-jest": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
